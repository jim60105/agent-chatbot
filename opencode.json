{
  "$schema": "https://opencode.ai/config.json",
  "provider": {
    "github": {
      "models": {},
      "options": {
        "apiKey": "{env:GITHUB_TOKEN}"
      }
    },
    "gemini": {
      "models": {},
      "options": {
        "apiKey": "{env:GEMINI_API_KEY}"
      }
    },
    "openrouter": {
      "models": {
        "deepseek/deepseek-v3.2": {}
      },
      "options": {
        "apiKey": "{env:OPENROUTER_API_KEY}"
      }
    }
  },
  "autoupdate": false,
  "compaction": {
    "auto": true,
    "prune": true
  }
}
